{
    "name": "pvbid-sdk",
    "version": "1.1.17",
    "description": "",
    "main": "dist/pvbid.node.js",
    "scripts": {
        "build": "npm run test && npm run docs && PLATFORM=node webpack && PLATFORM=web webpack",
        "build-node": "PLATFORM=node webpack",
        "build-web": "PLATFORM=web webpack",
        "watch": "SKIP_MIN=true WATCH=true webpack",
        "watch-node": "SKIP_MIN=true WATCH=true PLATFORM=node webpack",
        "docs": "./node_modules/.bin/esdoc && open docs/index.html",
        "publish-docs": "npm run test && npm run docs && PLATFORM=web DOCS=true webpack",
        "size": "size-limit",
        "test": "./node_modules/.bin/jest"
    },
    "size-limit": [
        {
            "path": "src/pvbid.js",
            "limit": "120 KB"
        }
    ],
    "jest": {
        "testRegex": "(/tests/.*(\\.|/)(test|spec))\\.js?$",
        "verbose": true,
        "browser": false,
        "testEnvironment": "node"
    },
    "keywords": [
        "pvbid",
        "solar",
        "javascript",
        "estimating",
        "bidding"
    ],
    "author": "PVBid, Inc",
    "license": "LicenseRef-LICENSE",
    "devDependencies": {
        "axios-mock-adapter": "^1.9.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^23.4.2",
        "babel-loader": "^7.1.2",
        "babel-minify-webpack-plugin": "^0.2.0",
        "babel-plugin-lodash": "^3.2.11",
        "babel-plugin-syntax-async-functions": "^6.13.0",
        "babel-plugin-transform-regenerator": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-1": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "compression-webpack-plugin": "^1.0.0",
        "dotenv": "^4.0.0",
        "esdoc": "^1.0.2",
        "esdoc-exclude-source-plugin": "^1.0.0",
        "esdoc-standard-plugin": "^1.0.0",
        "eslint": "^5.12.1",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jest": "^22.2.2",
        "eslint-plugin-node": "^8.0.0",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-standard": "^4.0.0",
        "happypack": "^4.0.0",
        "jest": "^23.4.2",
        "jest-environment-node-debug": "^2.0.0",
        "jsdoc-template": "github:braintree/jsdoc-template",
        "jsonfile": "^4.0.0",
        "lodash-webpack-plugin": "^0.11.4",
        "minami": "^1.2.3",
        "path": "^0.12.7",
        "size-limit": "^0.11.4",
        "webpack": "^3.5.5",
        "webpack-s3-plugin": "^1.0.0-rc.0"
    },
    "dependencies": {
        "axios": "^0.16.2",
        "eventemitter3": "^2.0.3",
        "lodash": "^4.17.4",
        "mathjs": "^3.16.3",
        "performance-now": "^2.1.0"
    }
}
