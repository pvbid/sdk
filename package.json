{
  "name": "pvbid-sdk",
  "version": "1.2.13",
  "description": "",
  "main": "dist/pvbid.node.js",
  "scripts": {
    "build": "npm run test && npm run docs && npm run build-node && npm run build-web",
    "build-node": "PLATFORM=node webpack --mode=production",
    "build-web": "PLATFORM=web webpack --mode=production",
    "watch": "WATCH=true webpack --mode=development",
    "watch-node": "WATCH=true PLATFORM=node webpack --mode=development",
    "docs": "./node_modules/.bin/esdoc && xdg-open docs/index.html",
    "publish-docs": "npm run test && npm run docs && PLATFORM=web DOCS=true webpack",
    "size": "size-limit",
    "test": "jest",
    "preversion": "npm run test",
    "version": "npm run build-node && npm run build-web",
    "postversion": "git push"
  },
  "size-limit": [
    {
      "path": "src/pvbid.js",
      "limit": "120 KB"
    }
  ],
  "jest": {
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": "babel-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "(/tests/.*(\\.|/)(test|spec))\\.(js|ts)?$",
    "verbose": true,
    "browser": false,
    "testEnvironment": "node"
  },
  "keywords": [
    "pvbid",
    "solar",
    "javascript",
    "estimating",
    "bidding"
  ],
  "author": "PVBid, Inc",
  "license": "LicenseRef-LICENSE",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.3.2",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.4",
    "@babel/runtime-corejs2": "^7.4.4",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.130",
    "axios-mock-adapter": "^1.9.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "compression-webpack-plugin": "^1.0.0",
    "dotenv": "^4.0.0",
    "esdoc": "^1.0.2",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-exclude-source-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "jest-environment-node-debug": "^2.0.0",
    "jsdoc-template": "github:braintree/jsdoc-template",
    "jsonfile": "^4.0.0",
    "lodash-webpack-plugin": "^0.11.4",
    "minami": "^1.2.3",
    "path": "^0.12.7",
    "size-limit": "^0.11.4",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-s3-plugin": "^1.0.3"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "eventemitter3": "^4.0.0",
    "lodash": "^4.17.4",
    "mathjs": "^3.16.3",
    "performance-now": "^2.1.0"
  }
}
